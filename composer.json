{
  "name": "nyx-solutions/yii2-nyx-curl",
  "description": "Yii2 NYX cUrl",
  "type": "library",
  "keywords": ["yii2", "extension", "curl", "request", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "5.0.0",
  "authors": [
    {
      "name": "NYX Solutions",
      "email": "nyx@jsas.com.br"
    },
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    },
    {
      "name": "Breno Herculano",
      "email": "brenosouherculano@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1.0 <8.2",

    "nyx-solutions/yii2-nyx": "~5.0.0",
    "nyx-solutions/yii2-nyx-helpers": "~5.0.0",

    "php-curl-class/php-curl-class": "~9.6.0"
  },
  "autoload": {
    "psr-4": {
      "nyx\\request\\": "request/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/nyx-solutions/yii2-nyx-curl"
    }
  ]
}
