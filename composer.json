  "keywords": ["yii2", "extension", "curl", "request", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "4.0.0",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "nyx-solutions/yii2-nyx": "~2.0.0",
